{
	"$schema": "https://raw.githubusercontent.com/microsoft/fluentui-token-pipeline/docs/token.schema.json",
	"Meta": {
		"FluentUITokensVersion": 0
	},
	"Global": {
		"Color": {
			"RegularGradient": { "value": {
				"start": [0, 1],
				"end": [0, 0],
				"stops": [
					{ "position": 0, "value": "black" },
					{ "position": 1, "value": "#c0c0c0" }
				]
			}},
			"FixedGradient": { "value": {
				"start": [0, 1],
				"end": [0, 0],
				"stops": [
					{ "position": 0, "value": "black" },
					{ "position": 4, "value": "#c0c0c0" }
				],
				"stopsUnits": "pixels"
			}},
			"BottomRightFixedGradient": { "value": {
				"start": [1, 1],
				"end": [0, 0],
				"stops": [
					{ "position": 0, "value": "black" },
					{ "position": 4, "value": "#c0c0c0" }
				],
				"stopsUnits": "pixels"
			}},
			"TokenGradient": { "value": {
				"start": [0, 0],
				"end": [0, 1],
				"stops": [
					{ "position": 0, "aliasOf": "Global.Color.Brand.80" },
					{ "position": 1, "aliasOf": "Global.Color.Brand.50" }
				]
			}}
		}
	}
}
